{
  "name": "One Tab per Job for Matecat",
  "short_name": "One Tab",
  "version": "1.0.0.3",
  "description": "Reuses Chrome tab for existing Matecat job when called from ApSIC Xbench",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions" : [
    "tabs"
  ],
  "icons" : {
    "48" : "icon-48.png",
    "128" : "icon-128.png"
  },

  "manifest_version": 2
}